		<Setter Property="Width" Value="14" />
		<Setter Property="Margin" Value="{DynamicResource AppBarToggleButtonOverflowCheckMargin}" />
		<Setter Property="Opacity" Value="0" />
		<Setter Property="IsVisible" Value="False" />
	</Style>


